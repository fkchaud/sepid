<%= form_with model: @project_type, local: true do |form| %>

  <div class="form-group row">
    <%= form.label :name, 'Nombre:', class: 'col col-form-label' %>
    <div class="col-10">
      <%= form.text_field :name, class: 'form-control-plaintext', readonly: true %>
    </div>
  </div>
  
  <div class="form-group row">
    <%= form.label :purpose, 'Finalidad:', class: 'col col-form-label' %>
    <div class="col-10">
      <%= form.text_field :purpose, class: 'form-control-plaintext', readonly: true %>
    </div>
  </div>

  <div class="form-group row">
    <%= form.label :function, 'Función:', class: 'col col-form-label' %>
    <div class="col-10">
      <%= form.text_field :function, class: 'form-control-plaintext', readonly: true %>
    </div>
  </div>

  <div class="form-group row">
    <%= form.label :program, 'Programa:', class: 'col col-form-label' %>
    <div class="col-10">
      <%= form.text_field :program, class: 'form-control-plaintext', readonly: true %>
    </div>
  </div>

  <div class="form-group row">
    <%= form.label :activity, 'Actividad:', class: 'col col-form-label' %>
    <div class="col-10">
      <%= form.text_field :activity, class: 'form-control-plaintext', readonly: true %>
    </div>
  </div>

  <div class="form-group row">
    <%= form.label :financing, 'Financiación:', class: 'col col-form-label' %>
    <div class="col-10">
      <%= form.text_field :financing, class: 'form-control-plaintext', readonly: true %>
    </div>
  </div>

<% end %>